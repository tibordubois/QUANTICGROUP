    "def randomChoice(elements, num_choice):\n",
    "    res = set()\n",
    "    for i in range(num_choice):\n",
    "        chosen = random.choice(list(elements))\n",
    "        elements.discard(chosen)\n",
    "        res.add(chosen)\n",
    "    return res"
